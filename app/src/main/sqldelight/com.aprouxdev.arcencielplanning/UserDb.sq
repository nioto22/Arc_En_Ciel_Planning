CREATE TABLE UserDb (
id TEXT NOT NULL PRIMARY KEY,
name TEXT NOT NULL,
isAdmin INTEGER AS Boolean DEFAULT 1,
imageUrl TEXT,
teams TEXT
);


getUserDbById:
SELECT * FROM UserDb WHERE id = :id;

getAllUserDbs:
SELECT * FROM UserDb;

insertUserDb:
INSERT OR REPLACE INTO UserDb VALUES (?, ?, ?, ?, ?);

deleteUserDbById:
DELETE FROM UserDb
WHERE id = :id;