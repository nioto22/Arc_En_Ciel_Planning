CREATE TABLE EventDb (
id TEXT NOT NULL PRIMARY KEY,
date TEXT,
time TEXT,
team INTEGER NOT NULL,
users TEXT,
title TEXT NOT NULL,
description TEXT,
comments TEXT
);


getEventDBById:
SELECT * FROM EventDb WHERE id = :id;

getAllEventDBs:
SELECT * FROM EventDb;

insertOrReplaceEventDB:
INSERT OR REPLACE INTO EventDb VALUES ?;

deleteEventDBById:
DELETE FROM EventDb
WHERE id = :id;