CREATE TABLE EventDB (
id INTEGER NOT NULL PRIMARY KEY,
date TEXT,
time TEXT,
team INTEGER NOT NULL,
users TEXT,
title TEXT NOT NULL,
description TEXT,
comments TEXT
);


getEventDBById:
SELECT * FROM EventDB WHERE id = :id;

getAllEventDBs:
SELECT * FROM EventDB;

insertEventDB:
INSERT OR REPLACE INTO EventDB VALUES (?, ?, ?, ?, ?, ?, ?, ?);

deleteEventDBById:
DELETE FROM EventDB
WHERE id = :id;